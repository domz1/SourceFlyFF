	int cy	= 0;

	m_AddRemoveLock.Enter();

	if( m_uHead <= m_uTail ) {
		for( int i =m_uHead; i < m_uTail; i++, cy += 14 ) {
			CMyTrace::TextOutA( hDC, 4, cy, m_mts[i].szText, m_mts[i].fInverse );
