			CMyTrace::TextOutA( hDC, 4, cy, m_mts[i].szText, m_mts[i].fInverse );
		}
		for( i = 0; i < m_uTail; i++, cy += 14 ) {
			CMyTrace::TextOutA( hDC, 4, cy, m_mts[i].szText, m_mts[i].fInverse );
		}
	}

	if( m_szStatusBar[0] != '\0' )
	{
		CMyTrace::TextOutA( hDC, 0, nYStatusBar-14, "________________________________________________________________________________________________________________________________________________________________", 0 );
