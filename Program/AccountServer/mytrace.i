u_long CMyTrace::Key( LPCSTR szKey )
{
	char szLwr[256];
	strcpy( szLwr, szKey );
	strlwr( szLwr );

	u_long uKey		= 0, num;

	for( int i	= strlen( szLwr ), j = 0; i > 0 && j < 4; j++, i-- )
	{
		uKey	= uKey << 8;
		num		= szLwr[i-1];
		uKey |= num;
	}
	return uKey;
}

